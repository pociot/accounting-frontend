{
  "extends": "tslint-config-airbnb",
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js"
    ]
  },
  "rules": {
    "ordered-imports": false,
    "object-literal-sort-keys": false,
    "no-empty-interface": false,
    "no-console": false,
    "import-name": [ true, {
      "react": "React",
      "axios": "Axios"
    }]
  }
}
